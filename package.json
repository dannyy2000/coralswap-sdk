{
  "name": "@coralswap/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for CoralSwap -- contract-first AMM on Stellar/Soroban",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**/*"],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": ["coralswap", "stellar", "soroban", "dex", "defi", "amm", "sdk", "typescript"],
  "author": "CoralSwap Finance",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CoralSwap-Finance/coralswap-sdk.git"
  },
  "dependencies": {
    "@stellar/stellar-sdk": "^12.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
